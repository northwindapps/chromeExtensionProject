{
  "manifest_version": 3,
  "name": "Screenshot Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/hello_extensions.png",
      "48": "images/hello_extensions.png",
      "128": "images/hello_extensions.png"
    }
  },
  "host_permissions": ["https://extension-tips.glitch.me/*"]
}